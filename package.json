{
  "name": "pigskit-web",
  "version": "0.0.1",
  "description": "Pigskit web",
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf dist",
    "build": "npm run clear && ./build.bash",
    "start": "LOCAL=true node dist/server.js",
    "restart": "npm run build && npm run start",
    "build-server": "webpack --mode production --config webpack.config.server.js",
    "build-root": "npm run webpack-prod",
    "build-home": "MODULE=home npm run webpack-prod",
    "build-shop": "MODULE=shop npm run webpack-prod",
    "build-menu": "MODULE=menu npm run webpack-prod",
    "build-test": "MODULE=test npm run webpack-prod",
    "build-test-api": "MODULE=test-api npm run webpack-prod",
    "dev-root": "npm run webpack-dev",
    "dev-home": "MODULE=home npm run webpack-dev",
    "dev-shop": "MODULE=shop npm run webpack-dev",
    "dev-menu": "MODULE=menu npm run webpack-dev",
    "dev-test": "MODULE=test npm run webpack-dev",
    "dev-test-api": "MODULE=test-api npm run webpack-dev",
    "webpack-prod": "webpack --mode production --config webpack.config.prod.js",
    "webpack-dev": "REST_SERVER_PORT=8001 GRAPHQL_SERVER_PORT=8000 webpack-dev-server --config webpack.config.dev.js"
  },
  "author": "David",
  "private": true,
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.10.1",
    "apollo-boost": "^0.4.9",
    "axios": "^0.19.2",
    "clsx": "^1.1.1",
    "express": "^4.17.1",
    "graphql": "^15.1.0",
    "http-proxy": "^1.18.1",
    "lodash": "^4.17.20",
    "qrcode.react": "^1.0.0",
    "rc-tween-one": "^2.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-icons": "^3.10.0",
    "react-image-crop": "^8.6.4",
    "react-measure": "^2.3.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "ejs": "^3.0.1",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
